{
    "manifest_version": 3,
    "name": "Janka Finance",
    "version": "1.0.3",
    "description": "Opens a new tab to a specific webpage once every 24 hours or when the extension icon is clicked",
    "permissions": ["alarms", "storage"],
    "action": {
        "default_title": "Janka Finance",
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icons/money-bag16.png",
        "32": "icons/money-bag32.png",
        "48": "icons/money-bag48.png",
        "128": "icons/money-bag128.png"
    }
}
  